	<template name="wall" type="">
		<collidable>
			<body name="main">
				<box w="8" h="64">
					<category value="0"/>
				</box>
			</body>
		</collidable>
		<renderable>
			<drawlist>
				<color r="1.0" g="0.9" b="0.3" a="1.0"/>
				<polygon>
					<vertex x="-8" y="64"/>
					<vertex x="8" y="64"/>
					<vertex x="8" y="-64"/>
					<vertex x="-8" y="-64"/>
				</polygon>
			</drawlist>
		</renderable>
	</template>

	<entity name="wallA" type="wall">
		<!-- right wall -->
		<position x="128.0" y="0.0" angle="0"/>
	</entity>

	<entity name="wallB" type="wall">
		<!-- top wall -->
		<position x="0.0" y="128.0" angle="90"/>
	</entity>

	<entity name="wallC" type="wall">
		<!-- left wall -->
		<position x="-128.0" y="0.0" angle="180"/>
	</entity>

	<entity name="wallD" type="wall">
		<!-- bottom wall -->
		<position x="0.0" y="-128.0" angle="270"/>
	</entity>


	<!-- bullseye targets -->

	<template name="targetexplosion" type="">
		<explosion life="0.5">
			<corecolor>
				<color time="0.00" r="1.0" g="0.9" b="0.8" a="1.0"/>
				<color time="0.50" r="0.0" g="0.5" b="1.0" a="0.0"/>
			</corecolor>
			<corescale>
				<scale time="0.00" x="10.0" y="10.0" z="1.0"/>
				<scale time="0.50" x="5.0" y="5.0" z="1.0"/>
			</corescale>
			<halocolor>
				<color time="0.00" r="1.0" g="0.7" b="0.4" a="0.5"/>
				<color time="0.50" r="1.0" g="0.0" b="0.0" a="0.0"/>
			</halocolor>
			<haloscale>
				<scale time="0.00" x="10.0" y="10.0" z="1.0"/>
				<scale time="0.50" x="30.0" y="30.0" z="1.0"/>
			</haloscale>
		</explosion>
	</template>

	<template name="target" type="">
		<collidable>
			<body name="main">
				<circle radius="10">
					<density value="1"/>
					<friction value="0.5"/>
					<restitution value="0.5"/>
					<category value="3"/>
				</circle>
				<damping linear="0.5" angular="0.5"/>
			</body>
		</collidable>
		<renderable>
			<drawlist>
				<color r="1.0" g="0.2" b="0.1" a="1.0"/>
				<pushmatrix>
					<scale x="4" y="4"/>
					<calllist name="circle8"/>
				</pushmatrix>
				<color r="1.0" g="1.0" b="1.0" a="1.0"/>
				<pushmatrix>
					<scale x="7" y="7"/>
					<calllist name="circle12"/>
				</pushmatrix>
				<color r="1.0" g="0.2" b="0.1" a="1.0"/>
				<pushmatrix>
					<scale x="10" y="10"/>
					<calllist name="circle16"/>
				</pushmatrix>
			</drawlist>
		</renderable>
		<damagable health="10" spawnondeath="targetexplosion"/>
	</template>

	<template name="targetappear" type="">
		<explosion life="0.25">
			<corecolor>
				<color time="0.00" r="1.0" g="1.0" b="1.0" a="0.0"/>
				<color time="0.25" r="1.0" g="1.0" b="1.0" a="1.0"/>
			</corecolor>
			<corescale>
				<scale time="0.00" x="3.0" y="3.0" z="1.0"/>
				<scale time="0.25" x="10.0" y="10.0" z="1.0"/>
			</corescale>
			<halocolor>
				<color time="0.00" r="1.0" g="0.0" b="0.0" a="0.0"/>
				<color time="0.25" r="1.0" g="0.0" b="0.0" a="0.5"/>
			</halocolor>
			<haloscale>
				<scale time="0.00" x="20.0" y="20.0" z="1.0"/>
				<scale time="0.25" x="10.0" y="10.0" z="1.0"/>
			</haloscale>
		</explosion>
