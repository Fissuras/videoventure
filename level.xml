	<!-- bullseye targets -->

	<template name="targetexplosion" type="">
		<explosion life="1.0">
			<pushmatrix>
				<color>
					<linear time="0.2">
						<start r="1.0" g="1.0" b="1.0" a="1.0"/>
						<end   r="1.0" g="1.0" b="1.0" a="0.0"/>
