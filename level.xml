<?xml version="1.0" standalone="no"?>
<!-- Test Level -->
<world>
  
  <!-- draw lists -->

  <drawlist name="circle8">
    <!-- 8 vertex circle -->
    <polygon>
      <vertex x="1.000000" y="0.000000"/>
      <vertex x="0.707107" y="0.707107"/>
      <vertex x="0.000000" y="1.000000"/>
      <vertex x="-0.707107" y="0.707107"/>
      <vertex x="-1.000000" y="0.00000"/>
      <vertex x="-0.707107" y="-0.707107"/>
      <vertex x="0.000000" y="-1.00000"/>
      <vertex x="0.707107" y="-0.707107"/>
    </polygon>
  </drawlist>

  <drawlist name="circle12">
    <!-- 12 vertex circle -->
    <polygon>
      <vertex x="1.000000" y="0.000000"/>
      <vertex x="0.866025" y="0.500000"/>
      <vertex x="0.500000" y="0.866025"/>
      <vertex x="0.000000" y="1.000000"/>
      <vertex x="-0.500000" y="0.866025"/>
      <vertex x="-0.866025" y="0.500000"/>
      <vertex x="-1.000000" y="0.00000"/>
      <vertex x="-0.866025" y="-0.500000"/>
      <vertex x="-0.500000" y="-0.866025"/>
      <vertex x="0.000000" y="-1.00000"/>
      <vertex x="0.500000" y="-0.866025"/>
      <vertex x="0.866025" y="-0.500000"/>
    </polygon>
  </drawlist>

  <drawlist name="circle16">
    <!-- 16 vertex circle -->
    <polygon>
      <vertex x="1.000000" y="0.000000"/>
      <vertex x="0.923880" y="0.382683"/>
      <vertex x="0.707107" y="0.707107"/>
      <vertex x="0.382683" y="0.923880"/>
      <vertex x="0.000000" y="1.000000"/>
      <vertex x="-0.382683" y="0.923880"/>
      <vertex x="-0.707107" y="0.707107"/>
      <vertex x="-0.923880" y="0.382683"/>
      <vertex x="-1.000000" y="0.00000"/>
      <vertex x="-0.923880" y="-0.382683"/>
      <vertex x="-0.707107" y="-0.707107"/>
      <vertex x="-0.382683" y="-0.923880"/>
      <vertex x="0.000000" y="-1.00000"/>
      <vertex x="0.382683" y="-0.923880"/>
      <vertex x="0.707107" y="-0.707107"/>
      <vertex x="0.923880" y="-0.382683"/>
    </polygon>
  </drawlist>

  <drawlist name="bullseye">
    <!-- bullseye target -->
    <color r="1.0" g="0.2" b="0.1" a="1.0"/>
    <pushmatrix>
      <scale x="4" y="4"/>
      <calllist name="circle8"/>
    </pushmatrix>
    <color r="1.0" g="1.0" b="1.0" a="1.0"/>
    <pushmatrix>
      <scale x="7" y="7"/>
      <calllist name="circle12"/>
    </pushmatrix>
    <color r="1.0" g="0.2" b="0.1" a="1.0"/>
    <pushmatrix>
      <scale x="10" y="10"/>
      <calllist name="circle16"/>
    </pushmatrix>
  </drawlist>

  <drawlist name="player">
    <!-- player ship -->
    <color r="1.0" g="1.0" b="1.0" a="1.0"/>
    <triangle_strip>
      <vertex x="16" y="-16"/>
      <vertex x="16" y="-11"/>
      <vertex x="5" y="-16"/>
      <vertex x="6" y="-6"/>
      <vertex x="4" y="-13"/>
      <vertex x="6" y="1"/>
      <vertex x="3" y="-13"/>
      <vertex x="3" y="4"/>
      <vertex x="2" y="-16"/>
      <vertex x="3" y="16"/>
      <vertex x="0" y="-16"/>
      <vertex x="-3" y="16"/>
      <vertex x="-2" y="-16"/>
      <vertex x="-3" y="4"/>
      <vertex x="-3" y="-13"/>
      <vertex x="-6" y="1"/>
      <vertex x="-4" y="-13"/>
      <vertex x="-6" y="-6"/>
      <vertex x="-5" y="-16"/>
      <vertex x="-16" y="-11"/>
      <vertex x="-16" y="-16"/>
    </triangle_strip>
  </drawlist>
  
  <drawlist name="gunner">
    <!-- player gunner -->
    <color r="0.5" g="0.5" b="0.5" a="1.0"/>
    <triangles>
      <vertex x="0" y="4"/>
      <vertex x="2" y="1"/>
      <vertex x="-2" y="1"/>
    </triangles>
    <color r="1.0" g="1.0" b="1.0" a="1.0"/>
    <scale x="5" y="5"/>
    <calllist name="circle12"/>
  </drawlist>

  
