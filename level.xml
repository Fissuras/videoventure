				</color>
				<scale>
					<linear time="0.2">
						<start x="20.0" y="20.0"/>
						<end   x="0.0" y="0.0"/>
					</linear>
				</scale>
				<calllist name="circle16"/>
			</pushmatrix>
			<pushmatrix>
				<color>
					<linear time="0.1">
						<start r="1.0" g="1.0" b="1.0" a="0.1"/>
						<end   r="1.0" g="1.0" b="1.0" a="0.0"/>
					</linear>
				</color>
				<scale>
					<linear time="0.1">
						<start x="10.0" y="10.0"/>
						<end   x="100.0" y="100.0"/>
					</linear>
				</scale>
				<calllist name="circle16"/>
			</pushmatrix>
			<pushmatrix>
				<color>
					<linear time="0.50">
						<start r="1.0" g="0.9" b="0.8" a="1.0"/>
						<end   r="1.0" g="0.5" b="0.0" a="0.25"/>
					</linear>
					<linear time="0.50">
						<start r="1.0" g="0.5" b="0.0" a="0.25"/>
						<end   r="1.0" g="0.0" b="0.0" a="0.0"/>
					</linear>
				</color>
				<scale>
					<linear time="1.00">
						<start x="10.0" y="10.0"/>
						<end   x="0.0" y="0.0"/>
					</linear>
				</scale>
				<calllist name="circle16"/>
			</pushmatrix>
			<pushmatrix>
				<repeat count="6">
					<repeat count="6">
						<pushmatrix>
							<translate>
								<linear time="0.25">
									<start x="0" y="0"/>
									<end   x="10" y="0"/>
								</linear>
							</translate>
							<color>
								<linear time="0.25">
									<start r="1.0" g="0.7" b="0.4" a="0.5"/>
									<end   r="1.0" g="0.0" b="0.0" a="0.0"/>
								</linear>
							</color>
							<scale>
								<linear time="0.25">
									<start x="1.5" y="0.3"/>
									<end   x="0.5" y="0.3"/>
								</linear>
							</scale>
							<calllist name="circle16"/>
						</pushmatrix>
						<rotate angle="60"/>
					</repeat>
					<scale x="1.5" y="1.5"/>
					<rotate angle="20"/>
				</repeat>
